<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframe.org/schema/context"
	xmlns:p="http://www.springframe.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans	
	 http://www.springframework.org/schema/beans/spring-beans.xsd
	 http://www.springframework.org/schema/context
	 http//www.springframework.org/schema/context/spring-context.org">
	<!-- this is bean -->
		
	<bean class="com.springcore.collections.Emp" name="emp1">
	<property name="name" value="Karan "/>
	<property name="phones">
		<list>
			<value>988784925</value>
			<value>53242343</value>
			<value>53242343</value>
		</list>
	</property>
	
	<property name="address">
	<set>
	
		<value>Delhi</value>
		<value>Kanpur</value>
		<value>Kanpur</value>
		</set>
	</property>
	
	
	<property name="courses">
		<map>
			<entry key="java" value="2 month"/>
			<entry key="python" value="5 month"/>
		</map>
	</property>
	</bean>
</beans>